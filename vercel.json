{
    "version": 2,
    "builds": [
        {
            "src": "bot.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "250mb",
                "runtime": "python3.9",
                "installCommand": "pip install --no-cache-dir -r requirements.txt"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "bot.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "PYTHONUNBUFFERED": "1"
    }
} 